{
  "name": "Servicio Idiomas",
  "version": "1.0.0",
  "description": "Language Service to show SOLID principles",
  "main": "servicio-mal.js",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node clock-class.ts\"",
    "pull": "git pull",
    "push": "rm *~; git pull; git add . ; git commit -m 'Code improvement'; git push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:ci": "eslint . --ext .tsx,.ts",
    "generate-doc": "node_modules/.bin/jsdoc --configure jsdoc-conf.json --verbose"
  },
  "keywords": [
    "PAI",
    "OOP",
    "TypeScript",
    "classes",
    "SOLID"
  ],
